{
  "name": "duckdb-sample-data",
  "title": "DuckDB Sample Data Package",
  "description": "A sample dataset created by a Java application demonstrating DuckDB usage with category-based data points",
  "version": "1.0.0",
  "keywords": [
    "duckdb",
    "sample-data",
    "categories",
    "java"
  ],
  "license": "MIT",
  "contributors": [
    {
      "title": "Data Generator",
      "role": "author"
    }
  ],
  "sources": [
    {
      "title": "Generated by Main.java",
      "path": "Generated programmatically using mathematical formulas"
    }
  ],
  "resources": [
    {
      "name": "mydata",
      "title": "Sample Category Data",
      "description": "Generated dataset with three categories (A, B, C) containing calculated y-values based on x-coordinates",
      "path": "mydata.db",
      "format": "duckdb",
      "mediatype": "application/x-duckdb",
      "encoding": "utf-8",
      "schema": {
        "fields": [
          {
            "name": "category",
            "title": "Category",
            "description": "Category identifier (A, B, or C)",
            "type": "string",
            "constraints": {
              "required": true,
              "enum": ["A", "B", "C"]
            }
          },
          {
            "name": "x_value",
            "title": "X Value",
            "description": "X coordinate value ranging from 1 to 20",
            "type": "integer",
            "constraints": {
              "required": true,
              "minimum": 1,
              "maximum": 20
            }
          },
          {
            "name": "y_value",
            "title": "Y Value", 
            "description": "Calculated Y value using formula: random(5,15) + (categoryOffset * 3) + (0.2 * x)",
            "type": "number",
            "constraints": {
              "required": true,
              "minimum": 5.0
            }
          }
        ],
        "primaryKey": ["category", "x_value"]
      }
    }
  ],
  "profile": "tabular-data-package",
  "created": "2025-10-28",
  "stats": {
    "totalRecords": 60,
    "recordsPerCategory": 20,
    "categories": ["A", "B", "C"]
  },
  "custom": {
    "generation": {
      "formula": "y = random(5, 15) + (ord(category) - 65) * 3 + 0.2 * x",
      "categoryOffsets": {
        "A": 0,
        "B": 3, 
        "C": 6
      },
      "xRange": {
        "min": 1,
        "max": 20
      },
      "randomRange": {
        "min": 5.0,
        "max": 15.0
      }
    },
    "applications": [
      "Main.java - Java DuckDB application"
    ]
  }
}